<div  class='planetInfoDisplay <%=@name%>'>
  <div id='left'>
    <image src='planetImages/padded/<%=@image%>'></image>
  </div>
  <div id='planetInfo'>
    <h1><%=@name%></h1>
    <ul>  
      <li>Mass: <%=@mass%> Earths</li>
      <li>Volume: <%=@volume%> Earths</li>
      <li>Distance from Sun: <%=@distance%> Miles <%=@distance_au%> AU </li>
    </ul>
    <p><%=@description%></p>
    
  </div>
</div>